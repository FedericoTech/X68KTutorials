<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="LoadSprites" />
		<Option pch_mode="2" />
		<Option compiler="human68k_gcc" />
		<Build>
			<Target title="default">
				<Option output="default/LoadSprites" prefix_auto="1" extension_auto="0" />
				<Option type="1" />
				<Option compiler="human68k_gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
		</Build>
		<Linker>
			<Add option="-Wl,-q,-Map=$(TARGET_OUTPUT_FILE).map,--cref" />
		</Linker>
		<ExtraCommands>
			<Add after="human68k-objcopy -O xfile $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).X" />
		</ExtraCommands>
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions />
	</Project>
</CodeBlocks_project_file>
